@page "/report"
@inherits ReportBase

<h3 class="text-center">Report</h3>

@if (unknownObjects == null)
{
    <div>
        loading
    </div>
}
else
{
    
    <table class="table table-bordered">
        <thead>
            <tr>
                <th scope="col" class="text-center">Numeric</th>
                <th scope="col" class="text-center">Alphanumeric</th>
                <th scope="col" class="text-center">Float</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td class="text-center">@NumericPercentage.ToString("0.##")</td>
                <td class="text-center">@AlphaNumbericPercentage.ToString("0.##")</td>
                <td class="text-center">@FloatPercentage.ToString("0.##")</td>
            </tr>
        </tbody>
    </table>
    <div class="p-3 m-3">

        @foreach(UnknownObject ob in unknownObjects)
        {
            <div>
                @ob.Value - @ob.Type
            </div>
        }
    </div>
}
